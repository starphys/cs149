#include <stdio.h>

int main(void) {

    int x;
    int y;
    int z;
    int min;

    fscanf(stdin, " %d %d %d", &x, &y, &z);

    // Note to the grader: this is a joke but it also works so I left it in
    min = x < y ? ((x < z) ? x : z) : ((y < z) ? y : z);

    fprintf(stdout, "%d\n", min);

    return 0;
}